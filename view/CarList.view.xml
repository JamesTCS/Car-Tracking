<core:View 
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	controllerName="tcs.cartracker.controller.CarList">
	
	<Page title="vehicles" >
	    <headerContent>
	        <Button icon="sap-icon://add" press="addCar" />
	        
	    </headerContent>
	
        <List 
        id="list"
        items="{/vehicles}" >
            <StandardListItem
              title="{name}"
              counter="{miles}"
              press="carSelect" />
        </List>
    </Page>
	
</core:View>