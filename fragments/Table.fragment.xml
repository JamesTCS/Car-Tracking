<core:FragmentDefinition
	xmlns:core="sap.ui.core"
	xmlns="sap.m">

    <Table id="dataTable" >
        <headerToolbar>
          <Toolbar>
            <Label id="tableLabel" text="Last Oil Changes"></Label>
          </Toolbar>
        </headerToolbar>
        <columns>
          <Column>
            <Text text="Date" />
          </Column>
          <Column>
            <Text text="Miles" />
          </Column>
        </columns>
        <items>
          <ColumnListItem>
            <cells>
                <Text text="{vehicleDB>date}" />
                <Text text="{vehicleDB>miles}" />
            </cells>
          </ColumnListItem>
        </items>
    </Table>
</core:FragmentDefinition>
